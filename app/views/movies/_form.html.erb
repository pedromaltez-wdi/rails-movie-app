<%= form_for(@movie) do |f| %>
  <div class="field">
  	<%= f.label :title %><br />
  	<%= f.text_field :title %>
  </div>
  <div class="field">
  	<%= f.label :photo_url %><br />
  	<%= f.text_field :photo_url %>
  </div>
  <div class="actions">
  	<%= f.submit :class => 'btn btn-primary' %>
  </div>
<% end %>